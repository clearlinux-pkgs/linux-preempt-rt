+#define WRITE_ONCE(p, v) (ACCESS_ONCE(p) = (v))
+#endif
+
+#if LINUX_VERSION_CODE >= KERNEL_VERSION(3, 17, 0)
+#include "udp_tunnel/udp_tunnel_partial_compat.h"
