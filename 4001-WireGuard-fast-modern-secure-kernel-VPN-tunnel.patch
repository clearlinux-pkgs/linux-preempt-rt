+	st->r[0] = le32_to_cpup((__le32 *)&key[ 0]) & 0x0fffffff;
+	st->r[1] = le32_to_cpup((__le32 *)&key[ 4]) & 0x0ffffffc;
+	st->r[2] = le32_to_cpup((__le32 *)&key[ 8]) & 0x0ffffffc;
